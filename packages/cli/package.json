    "test-help": "node dist/index.cjs --help"
  },
  "devDependencies": {
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "cac": "^6.7.14",
    "picocolors": "^1.0.0",
    "tsup": "^6.7.0"
  }
